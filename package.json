		"eject": "craco eject",
		"convertIcons": "npx @svgr/cli src/assets/icons/raw --out-dir src/assets/icons --typescript"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
